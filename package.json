  "main": "index.js",
  "scripts": {
    "dev": "npm run db:migrate && concurrently --kill-others \"yarn nodemon-integration-ui\" \"yarn nodemon-next\"",
    "nodemon-integration-ui": "nodemon --watch integrations --ext tsx ./.thomas/build/buildIntegrationUi.js",
    "nodemon-next": "nodemon --watch server --watch integrations --ext ts,json --exec ts-node --project tsconfig.server.json server/index.ts",
    "next-build": "next build",
    "next-start": "next start",
    "start": "NODE_ENV=production ts-node --project tsconfig.server.json server/index.ts",
    "pm2-start": "pm2 start npm --name 'Thomas' -- start",
    "pm2-stop": "pm2 stop 'Thomas'",
    "db:migrate": "sequelize-cli db:migrate",
    "db:rollback": "sequelize-cli db:migrate:undo"
  },
