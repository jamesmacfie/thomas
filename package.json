{
  "name": "thomas",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "watch": {
    "build-integration-ui": {
      "patterns": [
        "integrations/**/*"
      ],
      "ignore": [
        "integrations/store/*",
        "integrations/store.ts",
        "integrations/server/*",
        "integrations/server.ts"
      ],
      "extensions": "tsx,ts"
    },
    "next-dev": {
      "patterns": [
        "integrations/**/config.json",
        "integrations/**/server/*",
        "integrations/**/server.ts"
      ],
      "ignore": [
        "integrations/**/widgets/*",
        "integrations/**/store/*",
        "integrations/**/store.ts"
      ],
      "extensions": "json,ts"
    }
  },
  "scripts": {
    "dev": "npm run db:migrate && concurrently --kill-others \"yarn watch-integration-ui\" \"yarn watch-next-dev\"",
    "watch-integration-ui": "npm-watch build-integration-ui",
    "build-integration-ui": "node .thomas/build/buildIntegrationUi.js",
    "watch-next-dev": "npm-watch next-dev",
    "next-dev": "ts-node --project tsconfig.server.json server/index.ts",
    "next-build": "next build",
    "next-start": "next start",
    "start": "NODE_ENV=production ts-node --project tsconfig.server.json server/index.ts",
    "pm2-start": "pm2 start npm --name 'Thomas' -- start",
    "pm2-stop": "pm2 stop 'Thomas'",
    "db:migrate": "sequelize-cli db:migrate",
    "db:rollback": "sequelize-cli db:migrate:undo"
  },
  "author": "James Macfie",
  "keywords": [],
  "license": "MIT",
