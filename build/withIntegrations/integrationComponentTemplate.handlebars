import { ElementType } from 'react';
import NotFound from 'components/notFound';
{{#each components }}
import {{componentImportName}} from 'integrations/{{integrationSlug}}/components/{{componentSlug}}'
{{/each}}

const cmp = (slug: string): ElementType => {
  switch (slug) {
    {{#each components }}
    case '{{integrationSlug}}_{{componentSlug}}':
      return {{componentImportName}}
    {{/each}}
  } 
  return NotFound;
};

export default cmp;
