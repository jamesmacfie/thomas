import { ElementType } from 'react';
import NotFound from 'components/notFound';
{{#each settings }}
import {{widgetImportName}} from 'integrations/{{integrationSlug}}/settings'
{{#if hasChildren}}
{{#each children }}
import {{widgetImportName}} from 'integrations/{{integrationSlug}}/settings/{{settingsPageSlug}}'
{{/each}}
{{/if}}
{{/each}}

const components: { [key: string]: ElementType } = {
{{#each settings }}
'{{integrationSlug}}':{{widgetImportName}},
{{/each}}
};

export const hasCmp = (slug: string) => !!components[slug] ? true : false;

const cmp = (slug: string): ElementType => {
const slugCmp = components[slug]
return !!slugCmp ? slugCmp : NotFound;
};

export default cmp;
